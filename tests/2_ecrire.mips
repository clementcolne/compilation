.data
BackSlachN: .asciiz "\n"
Vrai: .asciiz "vrai"
Faux: .asciiz "faux"
ErreurDivisionZero: .asciiz "Erreur : Division par zéro"
.text

main:

	#allocation mémoire pour les variables
	move $s7, $sp
	add $sp, $sp, 0

	#ecrire 5
	li $v0, 5
	move $a0, $v0
	li $v0, 1
	syscall

	#retour à la ligne
	li $v0, 4 
	la $a0, BackSlachN
	syscall

	#ecrire 55
	li $v0, 55
	move $a0, $v0
	li $v0, 1
	syscall

	#retour à la ligne
	li $v0, 4 
	la $a0, BackSlachN
	syscall


end:
	#Sortie de programme
	li $v0, 10
	syscall
erreurDivisionZero:
	li $v0, 4 
	la $a0, ErreurDivisionZero
	syscall
	#Sortie de programme
	li $v0, 10
	syscall

