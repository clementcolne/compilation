.data
BackSlachN: .asciiz "\n"
Vrai: .asciiz "vrai"
Faux: .asciiz "faux"
ErreurDivisionZero: .asciiz "Erreur : Division par zéro"
.text

main:

	#allocation mémoire pour les variables
	move $s7, $sp
	add $sp, $sp, -16

	#ecrire fct()
	jal fonction1

	move $a0, $v0
	li $v0, 1
	syscall

	#retour à la ligne
	li $v0, 4 
	la $a0, BackSlachN
	syscall


end:
	#Sortie de programme
	li $v0, 10
	syscall
erreurDivisionZero:
	li $v0, 4 
	la $a0, ErreurDivisionZero
	syscall
	#Sortie de programme
	li $v0, 10
	syscall
fonction1:
	sw $ra, -4($s7)
	# a = 15
	li $v0, 15
	sw $v0, 0($s7)

	#ecrire a
	lw $v0, 0($s7)
	move $a0, $v0
	li $v0, 1
	syscall

	#retour à la ligne
	li $v0, 4 
	la $a0, BackSlachN
	syscall

	li $v0, 17
	lw $ra, -4($s7)
	jr $ra


